{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "5.1.9",
  "packageManager": "pnpm@10.16.1",
  "description": "Enlightened OG Image generation for Nuxt.",
  "author": {
    "website": "https://harlanzw.com",
    "name": "Harlan Wilton",
    "url": "harlan@harlanzw.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/og-image",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/og-image.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./content": "./dist/content.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ],
      "content": [
        "./dist/content.d.mts"
      ]
    }
  },
  "files": [
    "dist",
    "types/virtual.d.ts"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "stub": "nuxt-build-module build --stub && nuxt-module-build prepare",
    "build": "pnpm stub && nuxt-module-build build && pnpm run build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module build",
    "client:dev": "nuxi dev client --port 3030",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "prepare:fixtures": "nuxi prepare test/fixtures/basic",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare client && pnpm run prepare:fixtures",
    "release": "pnpm build && bumpp && pnpm -r publish --no-git-checks",
    "typecheck": "tsc --noEmit",
    "test": "pnpm run prepare:fixtures && vitest",
    "test:chrome": "HAS_CHROME=true vitest"
  },
  "peerDependencies": {
    "@unhead/vue": "^2.0.5",
    "unstorage": "^1.15.0"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^2.6.3",
    "@nuxt/kit": "^4.1.2",
    "@resvg/resvg-js": "^2.6.2",
    "@resvg/resvg-wasm": "^2.6.2",
    "@unocss/core": "^66.5.1",
    "@unocss/preset-wind3": "^66.5.1",
    "chrome-launcher": "^1.2.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "execa": "^9.6.0",
    "image-size": "^2.0.2",
    "magic-string": "^0.30.19",
    "mocked-exports": "^0.1.1",
    "nuxt-site-config": "^3.2.5",
    "nypm": "^0.6.2",
    "ofetch": "^1.4.1",
    "ohash": "^2.0.11",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "playwright-core": "^1.55.0",
    "radix3": "^1.1.2",
    "satori": "^0.15.2",
    "satori-html": "^0.3.2",
    "sirv": "^3.0.2",
    "std-env": "^3.9.0",
    "strip-literal": "^3.0.0",
    "ufo": "^1.6.1",
    "unplugin": "^2.3.10",
    "unwasm": "^0.3.11",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@css-inline/css-inline": "^0.17.0",
    "@headlessui/vue": "^1.7.23",
    "@iconify-json/carbon": "^1.2.13",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/noto": "^1.2.7",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/tabler": "^1.2.22",
    "@img/sharp-linux-x64": "^0.34.3",
    "@nuxt/content": "^3.7.1",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/devtools-ui-kit": "^2.6.3",
    "@nuxt/icon": "^2.0.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^10.1.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@unocss/nuxt": "^66.5.1",
    "@unocss/preset-icons": "^66.5.1",
    "@unocss/runtime": "^66.5.1",
    "@vueuse/nuxt": "^13.9.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "happy-dom": "^18.0.1",
    "jest-image-snapshot": "^6.5.1",
    "nuxt": "^4.1.2",
    "playwright": "^1.55.0",
    "sass": "^1.92.1",
    "sharp": "^0.34.3",
    "typescript": "^5.9.2",
    "unocss": "^66.5.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "json-editor-vue",
      "sharp",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
