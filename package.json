{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "3.0.0-rc.25",
  "packageManager": "pnpm@8.14.0",
  "description": "Enlightened OG Image generation for Nuxt.",
  "author": {
    "website": "https://harlanzw.com",
    "name": "Harlan Wilton",
    "url": "harlan@harlanzw.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/og-image",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/og-image.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "virtual.d.ts"
  ],
  "scripts": {
    "stub": "nuxt-build-module build --stub && nuxt-module-build prepare",
    "build": "pnpm stub && nuxt-module-build build && pnpm run build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module build",
    "lint": "eslint . --fix",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare .playground && nuxi prepare client",
    "release": "pnpm build && bumpp && pnpm -r publish --no-git-checks",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@css-inline/css-inline": "^0.12.1",
    "@css-inline/css-inline-wasm": "^0.12.1",
    "@iconify-json/noto": "^1.1.18",
    "@nuxt/devtools-kit": "^1.0.6",
    "@nuxt/kit": "^3.9.1",
    "@resvg/resvg-js": "^2.6.0",
    "@resvg/resvg-wasm": "^2.6.0",
    "@unocss/core": "^0.58.3",
    "@unocss/preset-wind": "^0.58.3",
    "@vueuse/core": "^10.7.1",
    "chrome-launcher": "^1.1.0",
    "defu": "^6.1.4",
    "execa": "^8.0.1",
    "floating-vue": "5.0.3",
    "image-size": "^1.1.1",
    "nuxt-site-config": "^2.2.5",
    "nuxt-site-config-kit": "^2.2.5",
    "nypm": "^0.3.4",
    "ofetch": "^1.3.3",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "pkg-types": "^1.0.3",
    "playwright-core": "^1.40.1",
    "radix3": "^1.1.0",
    "satori": "0.10.11",
    "satori-html": "^0.3.2",
    "sirv": "^2.0.4",
    "std-env": "^3.7.0",
    "terminate": "^2.6.1",
    "ufo": "^1.3.2",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.1",
    "@img/sharp-linux-x64": "0.33.1",
    "@nuxt/content": "^2.10.0",
    "@nuxt/devtools": "1.0.6",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/test-utils": "3.9.0",
    "@nuxt/ui": "^2.11.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.0.0",
    "@nuxtjs/tailwindcss": "^6.10.3",
    "@unocss/nuxt": "^0.58.3",
    "bumpp": "^9.2.1",
    "eslint": "8.56.0",
    "jest-image-snapshot": "^6.4.0",
    "nuxt": "^3.9.1",
    "nuxt-icon": "0.6.8",
    "playwright": "^1.40.1",
    "sass": "^1.69.7",
    "sharp": "^0.33.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "build": {
    "externals": [
      "h3",
      "nitropack",
      "@vue/runtime-core",
      "#nuxt-og-image/components",
      "sharp"
    ]
  }
}
