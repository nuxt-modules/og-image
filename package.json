    "@antfu/eslint-config": "^0.36.0",
    "@nuxt/devtools-edge": "0.2.5-27981287.ed164c8",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/test-utils": "3.3.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/ws": "^8.5.4",
    "bumpp": "^9.0.0",
    "eslint": "8.36.0",
    "jest-image-snapshot": "^6.1.0",
    "nuxt": "^3.3.1",
    "puppeteer": "^19.7.5",
    "vitest": "^0.29.3"
