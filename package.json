{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "2.2.0",
  "packageManager": "pnpm@8.9.2",
  "description": "Enlightened OG Image generation for Nuxt.",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-og-image#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-og-image.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && pnpm build:module && pnpm build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module",
    "lint": "eslint . --fix",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare .playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest build"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "@resvg/resvg-js": "^2.6.0",
    "@resvg/resvg-wasm": "^2.6.0",
    "@types/fs-extra": "^11.0.3",
    "birpc": "0.2.14",
    "chalk": "^5.3.0",
    "chrome-launcher": "^1.1.0",
    "css-inline": "^0.11.0",
    "defu": "^6.1.3",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.1",
    "flatted": "^3.2.9",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "image-size": "^1.0.2",
    "launch-editor": "^2.6.1",
    "nuxt-site-config": "^1.5.5",
    "nuxt-site-config-kit": "^1.5.5",
    "nypm": "^0.3.3",
    "ofetch": "^1.3.3",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "playwright-core": "^1.39.0",
    "radix3": "^1.1.0",
    "satori": "0.10.9",
    "satori-html": "^0.3.2",
    "sirv": "^2.0.3",
    "std-env": "^3.4.3",
    "svg2png-wasm": "^1.4.1",
    "terminate": "^2.6.1",
    "tinyws": "^0.1.0",
    "twemoji": "^14.0.2",
    "ufo": "^1.3.1",
    "ws": "^8.14.2",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.29",
    "@nuxt/devtools": "1.0.0",
    "@nuxt/module-builder": "^0.5.2",
    "@nuxt/test-utils": "3.8.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/ws": "^8.5.8",
    "bumpp": "^9.2.0",
    "eslint": "8.52.0",
    "jest-image-snapshot": "^6.2.0",
    "nuxt": "^3.8.0",
    "nuxt-icon": "0.6.1",
    "playwright": "^1.39.0",
    "sass": "^1.69.5",
    "vitest": "^0.34.6"
  }
}
