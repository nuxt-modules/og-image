{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "3.0.0-rc.42",
  "packageManager": "pnpm@8.15.4",
  "description": "Enlightened OG Image generation for Nuxt.",
  "author": {
    "website": "https://harlanzw.com",
    "name": "Harlan Wilton",
    "url": "harlan@harlanzw.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://nuxtseo.com/og-image",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/og-image.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "virtual.d.ts"
  ],
  "scripts": {
    "stub": "nuxt-build-module build --stub && nuxt-module-build prepare",
    "build": "pnpm stub && nuxt-module-build build && pnpm run build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module build",
    "lint": "eslint . --fix",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare .playground && nuxi prepare client",
    "release": "pnpm build && bumpp && pnpm -r publish --no-git-checks",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@css-inline/css-inline": "0.13.0",
    "@css-inline/css-inline-wasm": "0.13.0",
    "@iconify-json/noto": "^1.1.18",
    "@nuxt/devtools-kit": "^1.0.8",
    "@nuxt/kit": "^3.10.3",
    "@resvg/resvg-js": "^2.6.0",
    "@resvg/resvg-wasm": "^2.6.0",
    "@unocss/core": "^0.58.5",
    "@unocss/preset-wind": "^0.58.5",
    "@vueuse/core": "^10.9.0",
    "chrome-launcher": "^1.1.0",
    "defu": "^6.1.4",
    "execa": "^8.0.1",
    "flatted": "^3.3.1",
    "floating-vue": "5.2.2",
    "image-size": "^1.1.1",
    "json-editor-vue": "^0.12.0",
    "nuxt-icon": "^0.6.9",
    "nuxt-site-config": "^2.2.11",
    "nuxt-site-config-kit": "^2.2.11",
    "nypm": "^0.3.8",
    "ofetch": "^1.3.3",
    "ohash": "^1.1.3",
    "pathe": "^1.1.2",
    "pkg-types": "^1.0.3",
    "playwright-core": "^1.42.1",
    "radix3": "^1.1.0",
    "satori": "0.10.13",
    "satori-html": "^0.3.2",
    "shiki": "^1.1.7",
    "sirv": "^2.0.4",
    "splitpanes": "^3.1.5",
    "std-env": "^3.7.0",
    "terminate": "^2.6.1",
    "ufo": "^1.4.0",
    "unwasm": "^0.3.7",
    "vanilla-jsoneditor": "^0.22.0",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.8.0",
    "@headlessui/vue": "^1.7.19",
    "@iconify-json/carbon": "^1.1.31",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/ri": "^1.1.20",
    "@iconify-json/tabler": "^1.1.106",
    "@img/sharp-linux-x64": "0.33.2",
    "@nuxt/content": "^2.12.1",
    "@nuxt/devtools": "1.0.8",
    "@nuxt/devtools-ui-kit": "^1.0.8",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/test-utils": "3.11.0",
    "@nuxt/ui": "^2.14.2",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.1.1",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@unocss/nuxt": "^0.58.5",
    "@unocss/preset-icons": "^0.58.5",
    "@unocss/preset-uno": "^0.58.5",
    "@unocss/runtime": "^0.58.5",
    "@vueuse/nuxt": "^10.9.0",
    "bumpp": "^9.4.0",
    "eslint": "8.57.0",
    "jest-image-snapshot": "^6.4.0",
    "nuxt": "^3.10.3",
    "nuxt-icon": "0.6.9",
    "playwright": "^1.42.1",
    "sass": "^1.71.1",
    "sharp": "^0.33.2",
    "typescript": "^5.4.2",
    "unocss": "^0.58.5",
    "vitest": "^1.3.1"
  },
  "build": {
    "externals": [
      "h3",
      "nitropack",
      "@vue/runtime-core",
      "#nuxt-og-image/components",
      "sharp",
      "unstorage",
      "unstorage/drivers/fs"
    ]
  }
}
