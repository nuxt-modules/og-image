{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "2.0.13",
  "packageManager": "pnpm@8.6.12",
  "description": "Enlightened OG Image generation for Nuxt.",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-og-image#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-og-image.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && pnpm build:module && pnpm build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module",
    "lint": "eslint \"**/*.{ts,vue,json,yml}\" --fix",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare .playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "@resvg/resvg-js": "^2.4.1",
    "@resvg/resvg-wasm": "^2.4.1",
    "@types/fs-extra": "^11.0.1",
    "birpc": "^0.2.12",
    "chalk": "^5.3.0",
    "chrome-launcher": "^1.0.0",
    "defu": "^6.1.2",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.1",
    "flatted": "^3.2.7",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "image-size": "^1.0.2",
    "inline-css": "^4.0.2",
    "launch-editor": "^2.6.0",
    "nuxt-site-config": "^1.0.10",
    "nuxt-site-config-kit": "^1.0.10",
    "nypm": "^0.2.2",
    "ofetch": "^1.1.1",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "playwright-core": "^1.36.2",
    "radix3": "^1.0.1",
    "satori": "0.10.2",
    "satori-html": "^0.3.2",
    "sirv": "^2.0.3",
    "std-env": "^3.3.3",
    "svg2png-wasm": "^1.4.0",
    "tinyws": "^0.1.0",
    "twemoji": "^14.0.2",
    "ufo": "^1.2.0",
    "ws": "^8.13.0",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@nuxt/devtools-edge": "0.7.4-28188711.761b349",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/test-utils": "3.6.5",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/ws": "^8.5.5",
    "bumpp": "^9.1.1",
    "eslint": "8.46.0",
    "jest-image-snapshot": "^6.2.0",
    "nuxt": "^3.6.5",
    "nuxt-icon": "^0.5.0",
    "playwright": "^1.36.2",
    "sass": "^1.65.1",
    "vitest": "^0.34.1"
  },
  "resolutions": {
    "@nuxt/schema": "3.6.2"
  }
}
