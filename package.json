{
  "name": "nuxt-og-image",
  "type": "module",
  "version": "3.0.0-beta.42",
  "packageManager": "pnpm@8.11.0",
  "description": "Enlightened OG Image generation for Nuxt.",
  "author": {
    "website": "https://harlanzw.com",
    "name": "Harlan Wilton",
    "url": "harlan@harlanzw.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-og-image#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-og-image.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-og-image/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "virtual.d.ts"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && pnpm build:module && pnpm build:client",
    "build:client": "nuxi generate client",
    "build:module": "nuxt-build-module build",
    "lint": "eslint . --fix",
    "dev": "nuxi dev .playground",
    "dev:build": "nuxi build .playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare .playground",
    "release": "bumpp package.json --commit --push --tag",
    "test": "vitest"
  },
  "dependencies": {
    "@iconify-json/noto": "^1.1.17",
    "@nuxt/devtools-kit": "^1.0.4",
    "@nuxt/kit": "^3.8.2",
    "@resvg/resvg-js": "^2.6.0",
    "@resvg/resvg-wasm": "^2.6.0",
    "@unocss/core": "^0.58.0",
    "@unocss/preset-wind": "^0.58.0",
    "@vueuse/core": "^10.7.0",
    "chrome-launcher": "^1.1.0",
    "css-inline": "^0.11.0",
    "defu": "^6.1.3",
    "execa": "^8.0.1",
    "floating-vue": "2.0.0-beta.24",
    "image-size": "^1.0.2",
    "nuxt-site-config": "^1.6.6",
    "nuxt-site-config-kit": "^1.6.6",
    "nypm": "^0.3.3",
    "ofetch": "^1.3.3",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "playwright-core": "^1.40.1",
    "radix3": "^1.1.0",
    "satori": "0.10.11",
    "satori-html": "^0.3.2",
    "sirv": "^2.0.3",
    "std-env": "^3.6.0",
    "terminate": "^2.6.1",
    "ufo": "^1.3.2",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "sharp": "0.33.0",
    "@antfu/eslint-config": "2.3.4",
    "@img/sharp-linux-x64": "0.33.0",
    "@nuxt/content": "^2.9.0",
    "@nuxt/devtools": "1.0.4",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/test-utils": "3.8.1",
    "@nuxt/ui": "^2.11.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "8.0.0-rc.7",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@unocss/nuxt": "^0.58.0",
    "bumpp": "^9.2.0",
    "eslint": "8.55.0",
    "jest-image-snapshot": "^6.3.0",
    "nuxt": "^3.8.2",
    "nuxt-icon": "0.6.6",
    "playwright": "^1.40.1",
    "sass": "^1.69.5",
    "vitest": "^1.0.1"
  },
  "build": {
    "externals": [
      "h3",
      "nitropack",
      "@vue/runtime-core",
      "#nuxt-og-image/components"
    ]
  }
}
